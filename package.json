{
  "name": "@skyux-sdk/angular-builders",
  "version": "5.0.0-alpha.0",
  "repository": {
    "url": "https://github.com/blackbaud/skyux-sdk-angular-builders"
  },
  "license": "MIT",
  "builders": "builders.json",
  "schematics": "./collection.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "scripts": {
    "build": "npm run clean && npm run lint && tsc -p tsconfig.build.json",
    "postbuild": "node ./scripts/after-build-success.js",
    "clean": "rimraf dist",
    "e2e": "node ./scripts/e2e.js",
    "lint": "eslint src --ext .ts",
    "format-all": "npx prettier --write .",
    "start": "npm run clean && tsc-watch -p tsconfig.build.json --onSuccess \"npm run postbuild\"",
    "test": "nyc npm run test:unit",
    "test:unit": "npm run lint && ts-node --project tsconfig.json node_modules/jasmine/bin/jasmine.js --config=jasmine.json"
  },
  "peerDependencies": {
    "@angular-devkit/build-angular": "^12.0.0"
  },
  "dependencies": {
    "cross-spawn": "7.0.3",
    "fs-extra": "9.1.0",
    "glob": "7.1.6",
    "lodash.mergewith": "4.6.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1200.2",
    "@angular-devkit/build-angular": "12.0.2",
    "@angular-devkit/core": "12.0.2",
    "@angular-devkit/schematics": "12.0.2",
    "@angular/compiler": "12.0.2",
    "@angular/compiler-cli": "12.0.2",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@schematics/angular": "12.0.2",
    "@trivago/prettier-plugin-sort-imports": "2.0.2",
    "@types/cors": "2.8.10",
    "@types/cross-spawn": "6.0.2",
    "@types/fs-extra": "9.0.11",
    "@types/jasmine": "3.6.11",
    "@types/karma": "5.0.1",
    "@types/lodash.mergewith": "4.6.6",
    "@types/mock-require": "2.0.0",
    "@types/node": "14.14.45",
    "@typescript-eslint/eslint-plugin": "4.24.0",
    "@typescript-eslint/parser": "4.24.0",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "jasmine": "3.6.4",
    "mock-require": "3.0.3",
    "nyc": "15.1.0",
    "prettier": "2.3.0",
    "rimraf": "3.0.2",
    "ts-node": "8.3.0",
    "tsc-watch": "4.2.9",
    "typescript": "4.2.4"
  }
}
