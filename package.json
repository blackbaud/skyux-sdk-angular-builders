{
  "name": "@skyux-sdk/angular-builders",
  "version": "4.0.0-alpha.2",
  "repository": {
    "url": "https://github.com/blackbaud/skyux-sdk-angular-builders"
  },
  "license": "MIT",
  "builders": "builders.json",
  "schematics": "./collection.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "files": [
    "builders.json",
    "collection.json",
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && tsc --project tsconfig.json",
    "ci": "npm run coverage && npm run build",
    "clean": "rimraf dist",
    "coverage": "nyc npm run test",
    "lint": "tslint --project tsconfig.json",
    "start": "npm run build -- --watch",
    "test": "npm run clean && npm run lint && ts-node --project tsconfig.test.json node_modules/jasmine/bin/jasmine --config=jasmine.json"
  },
  "dependencies": {
    "fs-extra": "9.0.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1000.6",
    "@angular-devkit/core": "10.0.8",
    "@angular-devkit/schematics": "10.0.8",
    "@schematics/angular": "10.0.8",
    "@types/fs-extra": "9.0.1",
    "@types/jasmine": "3.5.14",
    "@types/node": "14.6.1",
    "jasmine": "3.6.1",
    "nyc": "15.1.0",
    "rewiremock": "3.14.3",
    "rimraf": "3.0.2",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "4.0.2"
  }
}
