{
  "name": "@skyux-sdk/angular-builders",
  "builders": "builders.json",
  "scripts": {
    "build": "rimraf dist && node ./node_modules/.bin/tsc -p tsconfig.json && node ./scripts/prepare-package.js",
    "build:watch": "npm run build && node ./node_modules/.bin/tsc -p tsconfig.json --watch",
    "ci": "npm run lint && npm run build",
    "lint": "tslint 'src/**/*.ts'"
  },
  "dependencies": {
    "html-webpack-plugin": "3.2.0",
    "os": "0.1.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.803.21",
    "@angular-devkit/build-angular": "0.803.21",
    "@angular-devkit/core": "8.3.21",
    "@angular/compiler": "8.2.14",
    "@angular/compiler-cli": "8.2.14",
    "@angular/core": "8.2.14",
    "@types/jasmine": "3.5.0",
    "@types/node": "12.12.14",
    "@types/webpack": "4.41.1",
    "@types/webpack-merge": "4.1.5",
    "fs-extra": "8.1.0",
    "open": "7.0.0",
    "rimraf": "3.0.0",
    "typescript": "3.5.3",
    "webpack": "4.41.2",
    "webpack-merge": "4.2.2",
    "zone.js": "0.9.1"
  }
}
