{
  "name": "@skyux-sdk/angular-builders",
  "version": "4.0.0-alpha.2",
  "repository": {
    "url": "https://github.com/blackbaud/skyux-sdk-angular-builders"
  },
  "license": "MIT",
  "builders": "builders.json",
  "schematics": "./collection.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "files": [
    "builders.json",
    "collection.json",
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run lint && tsc -p tsconfig.json",
    "postbuild": "node ./scripts/after-build-success.js",
    "ci": "npm run test && npm run build",
    "clean": "rimraf dist",
    "lint": "tslint 'src/**/*.ts'",
    "start": "npm run clean && tsc-watch -p tsconfig.json --onSuccess \"npm run postbuild\"",
    "test": "npm run lint"
  },
  "dependencies": {
    "fs-extra": "9.0.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1000.6",
    "@angular-devkit/core": "10.0.8",
    "@angular-devkit/schematics": "10.0.8",
    "@schematics/angular": "10.0.8",
    "@types/fs-extra": "9.0.1",
    "@types/jasmine": "3.5.14",
    "@types/node": "14.6.1",
    "rimraf": "3.0.2",
    "tsc-watch": "4.2.9",
    "tslint": "6.1.3",
    "typescript": "4.0.2"
  }
}
