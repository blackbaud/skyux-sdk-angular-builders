{
  "name": "blackbaud-bobbyearl-angular-builders",
  "version": "0.0.1-alpha.7",
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.json && node ./scripts/prepare-package.js",
    "clean": "rimraf dist",
    "start": "npm run build && tsc -p tsconfig.json --watch",
    "ci": "npm run lint && npm run build",
    "lint": "tslint 'src/**/*.ts'"
  },
  "builders": "builders.json",
  "schematics": "./collection.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "dependencies": {
    "@types/hash-file": "3.0.0",
    "hash-file": "3.0.0",
    "open": "7.1.0"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1000.6",
    "@angular-devkit/build-angular": "0.1000.6",
    "@angular-devkit/core": "10.0.6",
    "@angular-devkit/schematics": "10.0.7",
    "@angular/compiler": "10.0.9",
    "@angular/compiler-cli": "10.0.9",
    "@schematics/angular": "10.0.7",
    "@types/jasmine": "2.8.7",
    "@types/node": "12.12.2",
    "@types/webpack": "4.1.4",
    "fs-extra": "9.0.1",
    "rimraf": "3.0.0",
    "typescript": "3.9.7",
    "webpack": "4.44.1"
  }
}
