{
  "name": "@skyux-sdk/angular-builders",
  "version": "4.0.0-alpha.32",
  "repository": {
    "url": "https://github.com/blackbaud/skyux-sdk-angular-builders"
  },
  "license": "MIT",
  "builders": "builders.json",
  "schematics": "./collection.json",
  "ng-add": {
    "save": "devDependencies"
  },
  "scripts": {
    "build": "npm run clean && npm run lint && tsc -p tsconfig.json",
    "postbuild": "node ./scripts/after-build-success.js",
    "ci": "npm run coverage && npm run build",
    "clean": "rimraf dist",
    "coverage": "nyc npm run test",
    "lint": "tslint --project tsconfig.json",
    "format-all": "npx prettier --write .",
    "start": "npm run clean && tsc-watch -p tsconfig.json --onSuccess \"npm run postbuild\"",
    "test": "npm run lint && ts-node --project tsconfig.test.json node_modules/jasmine/bin/jasmine.js --config=jasmine.json"
  },
  "peerDependencies": {
    "@angular-devkit/build-angular": "^0.1102.6"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.17.1",
    "glob": "7.1.6",
    "hasha": "5.2.2",
    "lodash.merge": "4.6.2",
    "lodash.mergewith": "4.6.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1102.6",
    "@angular-devkit/build-angular": "0.1102.6",
    "@angular-devkit/core": "11.2.6",
    "@angular-devkit/schematics": "11.2.6",
    "@angular/compiler": "11.2.7",
    "@angular/compiler-cli": "11.2.7",
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@schematics/angular": "11.2.6",
    "@types/cors": "2.8.10",
    "@types/express": "4.17.11",
    "@types/fs-extra": "9.0.8",
    "@types/jasmine": "3.6.9",
    "@types/karma": "5.0.1",
    "@types/loader-utils": "2.0.2",
    "@types/lodash.merge": "4.6.6",
    "@types/lodash.mergewith": "4.6.6",
    "@types/mock-require": "2.0.0",
    "@types/node": "12.12.6",
    "@types/webpack": "4.41.26",
    "fs-extra": "9.1.0",
    "jasmine": "3.6.4",
    "jasmine-spec-reporter": "6.0.0",
    "loader-utils": "2.0.0",
    "mock-require": "3.0.3",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "protractor": "7.0.0",
    "rimraf": "3.0.2",
    "ts-node": "8.3.0",
    "tsc-watch": "4.2.9",
    "tslint": "6.1.3",
    "typescript": "4.1.5",
    "webpack": "4.44.2"
  }
}
